@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix : <http://crust.irk.ru/ontology/contents/terms/1.0/> .

#################################################################
# ДОПОЛНЕНИЯ К ОНТОЛОГИИ ДЛЯ КИМБЕРЛИТОВЫХ ТРУБОК
# Обобщенные структуры на основе Excel-базы данных
#################################################################

### 1. КИМБЕРЛИТОВАЯ ТРУБКА КАК ЦЕЛОСТНЫЙ ОБЪЕКТ ###

:KimberlitePipe a rdfs:Class ;
    rdfs:label "Kimberlite Pipe" ;
    rdfs:comment "Кимберлитовая трубка - вулканическое тело, сложенное кимберлитами и содержащее ксенолиты мантийных пород" ;
    rdfs:subClassOf :GeologicalBody .

:pipeId a rdf:Property ;
    rdfs:label "pipe identifier" ;
    rdfs:comment "Условный идентификатор трубки в базе данных (например, '1-1')" ;
    rdfs:domain :KimberlitePipe ;
    rdfs:range xsd:string .

### 2. ЦЕЛЕВЫЕ ПОКАЗАТЕЛИ (ABCDE) - ИНТЕГРАЛЬНАЯ ОЦЕНКА ###

:TargetIndicators a rdfs:Class ;
    rdfs:label "Target Indicators" ;
    rdfs:comment "Интегральные целевые показатели для оценки промышленной значимости трубки (параметры A, B, C, D, E)" .

:hasTargetIndicators a rdf:Property ;
    rdfs:label "has target indicators" ;
    rdfs:domain :KimberlitePipe ;
    rdfs:range :TargetIndicators .

:paramA a rdf:Property ;
    rdfs:label "parameter A" ;
    rdfs:comment "Целевой показатель A (секретный параметр)" ;
    rdfs:domain :TargetIndicators ;
    rdfs:range xsd:decimal .

:paramB a rdf:Property ;
    rdfs:label "parameter B" ;
    rdfs:comment "Целевой показатель B (секретный параметр)" ;
    rdfs:domain :TargetIndicators ;
    rdfs:range xsd:decimal .

:paramC a rdf:Property ;
    rdfs:label "parameter C" ;
    rdfs:comment "Целевой показатель C (секретный параметр)" ;
    rdfs:domain :TargetIndicators ;
    rdfs:range xsd:decimal .

:paramD a rdf:Property ;
    rdfs:label "parameter D" ;
    rdfs:comment "Целевой показатель D (секретный параметр)" ;
    rdfs:domain :TargetIndicators ;
    rdfs:range xsd:decimal .

:paramE a rdf:Property ;
    rdfs:label "parameter E" ;
    rdfs:comment "Целевой показатель E (секретный параметр, часто отсутствует)" ;
    rdfs:domain :TargetIndicators ;
    rdfs:range xsd:string .

### 3. ГЕОЛОГИЧЕСКИЕ ХАРАКТЕРИСТИКИ ТРУБКИ ###

:PipeGeology a rdfs:Class ;
    rdfs:label "Pipe Geology" ;
    rdfs:comment "Геологические характеристики кимберлитовой трубки" .

:hasGeology a rdf:Property ;
    rdfs:label "has geology" ;
    rdfs:domain :KimberlitePipe ;
    rdfs:range :PipeGeology .

:overburden a rdf:Property ;
    rdfs:label "overburden" ;
    rdfs:comment "Наличие перекрывающих отложений" ;
    rdfs:domain :PipeGeology ;
    rdfs:range xsd:string .  # "нет", "есть"

### 4. ПЕТРОГРАФИЧЕСКИЕ ХАРАКТЕРИСТИКИ ###

:Petrography a rdfs:Class ;
    rdfs:label "Petrography" ;
    rdfs:comment "Петрографические характеристики пород трубки" .

:hasPetrography a rdf:Property ;
    rdfs:label "has petrography" ;
    rdfs:domain :KimberlitePipe ;
    rdfs:range :Petrography .

:OlivineSizeFraction a rdfs:Class ;
    rdfs:label "Olivine Size Fraction" ;
    rdfs:comment "Размерная фракция оливинов I генерации" .

:fractionRange a rdf:Property ;
    rdfs:label "fraction range" ;
    rdfs:comment "Диапазон размеров фракции (например, '1-2 мм')" ;
    rdfs:domain :OlivineSizeFraction ;
    rdfs:range xsd:string .

:fractionPercentage a rdf:Property ;
    rdfs:label "fraction percentage" ;
    rdfs:comment "Процентное содержание данной фракции" ;
    rdfs:domain :OlivineSizeFraction ;
    rdfs:range xsd:decimal .

:hasOlivineFraction a rdf:Property ;
    rdfs:label "has olivine fraction" ;
    rdfs:comment "Связь петрографии с размерной фракцией оливинов" ;
    rdfs:domain :Petrography ;
    rdfs:range :OlivineSizeFraction .

### 5. АЛМАЗНАЯ АССОЦИАЦИЯ - ОБОБЩЕННАЯ СТРУКТУРА ###

:DiamondAssociation a rdfs:Class ;
    rdfs:label "Diamond Association" ;
    rdfs:comment "Показатели алмазной ассоциации для трубки (интегральные оценки по минералам-индикаторам)" .

:hasDiamondAssociation a rdf:Property ;
    rdfs:label "has diamond association" ;
    rdfs:domain :KimberlitePipe ;
    rdfs:range :DiamondAssociation .

# Классификация гранатов по Соболеву/Shulze
:GarnetDiamondClassification a rdfs:Class ;
    rdfs:label "Garnet Diamond Classification" ;
    rdfs:comment "Классификация гранатов по отношению к алмазоносности (по Соболеву 1974, Shulze 2003)" .

:hasGarnetClassification a rdf:Property ;
    rdfs:label "has garnet classification" ;
    rdfs:domain :DiamondAssociation ;
    rdfs:range :GarnetDiamondClassification .

:garSobolev1974Peridotitic a rdf:Property ;
    rdfs:label "gar Sobolev 1974 peridotitic %" ;
    rdfs:comment "Алмазная ассоциация гранатов (по Соболев 1974) % от перидотитовых гранатов (по Shulze 2003)" ;
    rdfs:domain :GarnetDiamondClassification ;
    rdfs:range xsd:decimal .

:g10Percent a rdf:Property ;
    rdfs:label "G10 %" ;
    rdfs:comment "Процент гранатов G10" ;
    rdfs:domain :GarnetDiamondClassification ;
    rdfs:range xsd:decimal .

:g10dPercent a rdf:Property ;
    rdfs:label "G10D %" ;
    rdfs:comment "Процент гранатов G10D" ;
    rdfs:domain :GarnetDiamondClassification ;
    rdfs:range xsd:decimal .

:g3dPercent a rdf:Property ;
    rdfs:label "G3D %" ;
    rdfs:comment "Процент гранатов G3D" ;
    rdfs:domain :GarnetDiamondClassification ;
    rdfs:range xsd:decimal .

:g4dPercent a rdf:Property ;
    rdfs:label "G4D %" ;
    rdfs:comment "Процент гранатов G4D" ;
    rdfs:domain :GarnetDiamondClassification ;
    rdfs:range xsd:decimal .

:g5dPercent a rdf:Property ;
    rdfs:label "G5D %" ;
    rdfs:comment "Процент гранатов G5D" ;
    rdfs:domain :GarnetDiamondClassification ;
    rdfs:range xsd:decimal .

:cr2o3gt5Percent a rdf:Property ;
    rdfs:label "Cr2O3 > 5 wt.% %" ;
    rdfs:comment "Процент зерен с Cr2O3 > 5 мас.%" ;
    rdfs:domain :GarnetDiamondClassification ;
    rdfs:range xsd:decimal .

:tio2Peridotitic a rdf:Property ;
    rdfs:label "TiO2 peridotitic wt.%" ;
    rdfs:comment "TiO2, мас.% (для перидотитовых гранатов)" ;
    rdfs:domain :GarnetDiamondClassification ;
    rdfs:range xsd:decimal .

:tio2HighCr a rdf:Property ;
    rdfs:label "TiO2 high-Cr wt.%" ;
    rdfs:comment "TiO2, мас.% (при Cr2O3 > 5 мас.%)" ;
    rdfs:domain :GarnetDiamondClassification ;
    rdfs:range xsd:decimal .

# Хромитовая алмазная ассоциация
:ChromiteDiamondAssociation a rdfs:Class ;
    rdfs:label "Chromite Diamond Association" ;
    rdfs:comment "Показатели алмазной ассоциации по хромитам" .

:hasChromiteAssociation a rdf:Property ;
    rdfs:label "has chromite association" ;
    rdfs:domain :DiamondAssociation ;
    rdfs:range :ChromiteDiamondAssociation .

:chromiteDiamondPercent a rdf:Property ;
    rdfs:label "chromite diamond association %" ;
    rdfs:comment "Алмазная ассоциация, % хромитов" ;
    rdfs:domain :ChromiteDiamondAssociation ;
    rdfs:range xsd:decimal .

:chromitePeridotiticTrendPercent a rdf:Property ;
    rdfs:label "chromite peridotitic trend %" ;
    rdfs:comment "% принадлежащих к перидотитовому тренду хромитов" ;
    rdfs:domain :ChromiteDiamondAssociation ;
    rdfs:range xsd:decimal .

# Ильменитовая классификация
:IlmeniteClassification a rdfs:Class ;
    rdfs:label "Ilmenite Classification" ;
    rdfs:comment "Классификация ильменитов по происхождению" .

:hasIlmeniteClassification a rdf:Property ;
    rdfs:label "has ilmenite classification" ;
    rdfs:domain :DiamondAssociation ;
    rdfs:range :IlmeniteClassification .

:kimberliticIlmenitePercent a rdf:Property ;
    rdfs:label "kimberlitic ilmenite %" ;
    rdfs:comment "Кимберлитовые ильмениты, %" ;
