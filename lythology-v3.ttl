:hasTypeValue a rdf:Property ;
    rdfs:label "has type value" ;
    rdfs:comment "Список значений по типам" ;
    rdfs:domain :MineralTypeComposition ;
    rdfs:range :MineralTypeValue .

:typeNumber a rdf:Property ;
    rdfs:label "type number" ;
    rdfs:comment "Номер типа (внутренняя классификация)" ;
    rdfs:domain :MineralTypeValue ;
    rdfs:range xsd:integer .

:percentage a rdf:Property ;
    rdfs:label "percentage" ;
    rdfs:comment "Процентное содержание данного типа" ;
    rdfs:domain :MineralTypeValue ;
    rdfs:range xsd:decimal .

