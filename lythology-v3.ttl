:nonKimberliticIlmenitePercent a rdf:Property ;
    rdfs:label "non-kimberlitic ilmenite %" ;
    rdfs:comment "Не кимберлитовые ильмениты, %" ;
    rdfs:domain :IlmeniteClassification ;
    rdfs:range xsd:decimal .

# Качественные показатели алмазоносности
:QualityIndicators a rdfs:Class ;
    rdfs:label "Quality Indicators" ;
    rdfs:comment "Качественные показатели алмазоносности" .

:hasQualityIndicators a rdf:Property ;
    rdfs:label "has quality indicators" ;
    rdfs:domain :DiamondAssociation ;
    rdfs:range :QualityIndicators .

:goodQualityPercent a rdf:Property ;
