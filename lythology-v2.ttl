    rdfs:range xsd:decimal .

:geothermalGradient_Gar a rdf:Property ;
    rdfs:label "geothermal gradient by Garnet (mW/m²)" ;
    rdfs:domain :IntegralIndicator ;
    rdfs:range xsd:decimal .

:lithosphereThickness_Gar a rdf:Property ;
    rdfs:label "lithosphere thickness by Garnet (km)" ;
    rdfs:domain :IntegralIndicator ;
    rdfs:range xsd:decimal .

:diamondWindowThickness_Gar a rdf:Property ;
