:spinel a gs:Mineral, dbp-owl:Mineral ;
    rdfs:label "Spinel" .

:olivine a gs:Mineral, dbp-owl:Mineral ;
    rdfs:label "Olivine" .

:clinopyroxene a gs:Mineral, dbp-owl:Mineral ;
