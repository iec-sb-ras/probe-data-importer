:orthopyroxene a gs:Mineral, dbp-owl:Mineral ;
    rdfs:label "Orthopyroxene" .

:ilmenite a gs:Mineral, dbp-owl:Mineral ;
    rdfs:label "Ilmenite" .

:phlogopite a gs:Mineral, dbp-owl:Mineral ;
    rdfs:label "Phlogopite" .

:amphibole a gs:Mineral, dbp-owl:Mineral ;
    rdfs:label "Amphibole" .

:biotite a gs:Mineral, dbp-owl:Mineral ;
    rdfs:label "Biotite" .

:chromite a gs:Mineral, dbp-owl:Mineral ;
    rdfs:label "Chromite" .

:kyanite a gs:Mineral, dbp-owl:Mineral ;
    rdfs:label "Kyanite" .

:diamond a gs:Mineral, dbp-owl:Mineral ;
