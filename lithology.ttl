### КЛАСС ДЛЯ ОСОБОГО ТИПА ОБРАЗЦА (MEGACRYST) ###

:megacryst a gs:GeologicUnit ;
    rdfs:label "Megacryst" ;
    rdfs:comment "An exceptionally large crystal, typically found in igneous rocks as a phenocryst." .


### ВИДЫ ИНКЛЮЗИЙ ###

:Primary a gs:Inclusion ;
    rdfs:label "Primary" .

:Secondary a gs:Secondary ;
    rdfs:label "Secondary" .

### КЛАССЫ ДЛЯ ТЕКСТУР ГОРНЫХ ПОРОД (ROCK TEXTURE) ###

# Основные категории текстур
:RockTexture a rdfs:Class ;
    rdfs:label "Rock Texture" ;
    rdfs:subClassOf gs:GeologicStructure .

# Породообразующие текстуры
:CoarseGrainedTexture a :RockTexture ;
    rdfs:label "Coarse-Grained" .

:MediumGrainedTexture a :RockTexture ;
    rdfs:label "Medium-Grained" .

:FineGrainedTexture a :RockTexture ;
    rdfs:label "Fine-Grained" .

# Породные текстуры
:GranoblasticTexture a :RockTexture ;
    rdfs:label "Granoblastic" ;
    rdfs:subClassOf gs:MetamorphicTexture .

:PorphyroclasticTexture a :RockTexture ;
    rdfs:label "Porphyroclastic" ;
    rdfs:subClassOf gs:MetamorphicTexture .

:PorphyriticTexture a :RockTexture ;
    rdfs:label "Porphyritic" ;
    rdfs:subClassOf gs:IgneousTexture .

:CumulateTexture a :RockTexture ;
    rdfs:label "Cumulate" ;
    rdfs:subClassOf gs:IgneousTexture .

:MegacrystallineTexture a :RockTexture ;
    rdfs:label "Megacrystalline" ;
    rdfs:subClassOf gs:IgneousTexture .

:GranularTexture a :RockTexture ;
    rdfs:label "Granular" .

:MosaicTexture a :RockTexture ;
    rdfs:label "Mosaic" .

:EquigranularTexture a :RockTexture ;
    rdfs:label "Equigranular" .

:ShearedTexture a :RockTexture ;
    rdfs:label "Sheared" ;
    rdfs:subClassOf gs:DeformationalTexture .

:FoliatedTexture a :RockTexture ;
    rdfs:label "Foliated" ;
    rdfs:subClassOf gs:MetamorphicTexture .

:FluidalTexture a :RockTexture ;
    rdfs:label "Fluidal" .

:BandedTexture a :RockTexture ;
    rdfs:label "Banded" .

:LaminatedTexture a :RockTexture ;
    rdfs:label "Laminated" .

# Комбинированные текстуры
:CoarsePorphyriticTexture a :RockTexture ;
    rdfs:label "Coarse Porphyritic" ;
    rdfs:subClassOf :CoarseGrainedTexture, :PorphyriticTexture .

:FinePorphyriticTexture a :RockTexture ;
    rdfs:label "Fine Porphyritic" ;
    rdfs:subClassOf :FineGrainedTexture, :PorphyriticTexture .

:CoarsePorphyroclasticTexture a :RockTexture ;
    rdfs:label "Coarse Porphyroclastic" ;
    rdfs:subClassOf :CoarseGrainedTexture, :PorphyroclasticTexture .

:FineGrainedPorphyroclasticTexture a :RockTexture ;
    rdfs:label "Fine-Grained Porphyroclastic" ;
    rdfs:subClassOf :FineGrainedTexture, :PorphyroclasticTexture .

:MosaicPorphyroclasticTexture a :RockTexture ;
    rdfs:label "Mosaic-Porphyroclastic" ;
    rdfs:subClassOf :MosaicTexture, :PorphyroclasticTexture .

:GranoblasticBandedTexture a :RockTexture ;
    rdfs:label "Granoblastic, Banded" ;
    rdfs:subClassOf :GranoblasticTexture, :BandedTexture .

:CoarseGranoblasticTexture a :RockTexture ;
    rdfs:label "Coarse Granoblastic" ;
    rdfs:subClassOf :CoarseGrainedTexture, :GranoblasticTexture .

:CoarseEquantTexture a :RockTexture ;
    rdfs:label "Coarse Equant" ;
    rdfs:subClassOf :CoarseGrainedTexture, :EquigranularTexture .

:CoarseTabularTexture a :RockTexture ;
    rdfs:label "Coarse-Tabular" ;
    rdfs:subClassOf :CoarseGrainedTexture .

:MediumToCoarseGrainedTexture a :RockTexture ;
    rdfs:label "Medium- to Coarse-Grained" ;
    rdfs:subClassOf :MediumGrainedTexture, :CoarseGrainedTexture .

:FineToMediumGrainedTexture a :RockTexture ;
    rdfs:label "Fine- to Medium-Grained" ;
    rdfs:subClassOf :FineGrainedTexture, :MediumGrainedTexture .

:HeterogeneousTexture a :RockTexture ;
    rdfs:label "Heterogeneous" .

:AllotriomorphicGranularTexture a :RockTexture ;
    rdfs:label "Allotriomorphic Granular" ;
    rdfs:subClassOf :GranularTexture .

:XenomorphicGranularTexture a :RockTexture ;
    rdfs:label "Xenomorphic Granular" ;
    rdfs:subClassOf :GranularTexture .

:InterlockingTexture a :RockTexture ;
    rdfs:label "Interlocking" .

:DeformedTexture a :RockTexture ;
    rdfs:label "Deformed" ;
    rdfs:subClassOf gs:DeformationalTexture .

:TransitionalTexture a :RockTexture ;
    rdfs:label "Transitional" .

:MyloniticTexture a :RockTexture ;
    rdfs:label "Mylonitic" ;
    rdfs:subClassOf gs:DeformationalTexture .

:FluidalMosaicTexture a :RockTexture ;
    rdfs:label "Fluidal Mosaic" ;
    rdfs:subClassOf :FluidalTexture, :MosaicTexture .

:LayeredDisruptedMosaicPorphyroclasticTexture a :RockTexture ;
    rdfs:label "Layered and Disrupted Mosaic Porphyroclastic" ;
    rdfs:subClassOf :MosaicPorphyroclasticTexture .

# Специфичные текстуры для ультрамафитов
:ProtogranularTexture a :RockTexture ;
    rdfs:label "Protogranular" ;
    rdfs:subClassOf :GranularTexture .

:CoarseProtogranularTexture a :RockTexture ;
    rdfs:label "Coarse Protogranular" ;
    rdfs:subClassOf :CoarseGrainedTexture, :ProtogranularTexture .

:CoarseLamellarTexture a :RockTexture ;
    rdfs:label "Coarse Lamellar" ;
    rdfs:subClassOf :CoarseGrainedTexture .

# Сложные комбинированные текстуры
:ComplexTexture a :RockTexture ;
    rdfs:label "Complex Texture" .

:HypautomorphicOphiticTexture a :RockTexture ;
    rdfs:label "Hypautomorphic, Ophitic with Transition to Grano-lapidoblastic" .

:FasciculateTexture a :RockTexture ;
    rdfs:label "Fasciculate" .

:PolygonalGranoblasticTexture a :RockTexture ;
    rdfs:label "Polygonal Granoblastic" ;
    rdfs:subClassOf :GranoblasticTexture .

### СВОЙСТВА ДЛЯ СВЯЗЕЙ ###

:hasTexture a rdf:Property ;
    rdfs:label "has texture" ;
    rdfs:domain gs:RockType ;
    rdfs:range :RockTexture .

:textureDescription a rdf:Property ;
    rdfs:label "texture description" ;
    rdfs:domain :RockTexture ;
    rdfs:range rdfs:Literal .

### НОВЫЕ КЛАССЫ ДЛЯ МЕТАДАННЫХ ###

# Типы альтерации
:AlterationType a rdfs:Class ;
    rdfs:label "Alteration Type" ;
    rdfs:subClassOf gs:GeologicProcess .

:Serpentinization a :AlterationType ;
    rdfs:label "Serpentinization" .

:Carbonatization a :AlterationType ;
    rdfs:label "Carbonatization" .

:Chloritization a :AlterationType ;
    rdfs:label "Chloritization" .

:Kaolinization a :AlterationType ;
    rdfs:label "Kaolinization" .

# Типы отбора проб
:SamplingEnvironment a rdfs:Class ;
    rdfs:label "Sampling Environment" .

:Subaerial a :SamplingEnvironment ;
    rdfs:label "Subaerial" .

:Submarine a :SamplingEnvironment ;
    rdfs:label "Submarine" .

:Underground a :SamplingEnvironment ;
    rdfs:label "Underground" .

# Типы включений
:InclusionType a rdfs:Class ;
    rdfs:label "Inclusion Type" .

:Primary a :InclusionType ;
    rdfs:label "Primary" .

:Secondary a :InclusionType ;
    rdfs:label "Secondary" .

# Геометрические объекты
:GeoBounds a rdfs:Class ;
    rdfs:label "Geographic Bounds" ;
    rdfs:subClassOf wgs:SpatialThing .

:latitudeMin a rdf:Property ;
    rdfs:label "minimum latitude" ;
    rdfs:domain :GeoBounds ;
    rdfs:range xsd:decimal .

:latitudeMax a rdf:Property ;
    rdfs:label "maximum latitude" ;
    rdfs:domain :GeoBounds ;
    rdfs:range xsd:decimal .

:longitudeMin a rdf:Property ;
    rdfs:label "minimum longitude" ;
    rdfs:domain :GeoBounds ;
    rdfs:range xsd:decimal .

:longitudeMax a rdf:Property ;
    rdfs:label "maximum longitude" ;
    rdfs:domain :GeoBounds ;
    rdfs:range xsd:decimal .

:elevationMin a rdf:Property ;
    rdfs:label "minimum elevation" ;
    rdfs:domain :GeoBounds ;
    rdfs:range xsd:decimal .

:elevationMax a rdf:Property ;
    rdfs:label "maximum elevation" ;
    rdfs:domain :GeoBounds ;
    rdfs:range xsd:decimal .

:locationComment a rdf:Property ;
    rdfs:label "location comment" ;
    rdfs:domain :GeoBounds ;
    rdfs:range xsd:string .

### СВОЙСТВА ДЛЯ СВЯЗЕЙ ###

:hasAlteration a rdf:Property ;
    rdfs:label "has alteration" ;
    rdfs:domain gs:RockType ;
    rdfs:range :AlterationType .

:samplingEnvironment a rdf:Property ;
    rdfs:label "sampling environment" ;
    rdfs:domain :Sample ;
    rdfs:range :SamplingEnvironment .

:inclusion a rdf:Property ;
    rdfs:label "inclusion type" ;
    rdfs:domain :Sample ;
    rdfs:range :InclusionType .
